{
  "BagIt-Profile-Info": {
    "BagIt-Profile-Identifier": "https://raw.githubusercontent.com/",
    "BagIt-Profile-Version": "",
    "Contact-Email": "noc@senylrc.org",
    "Contact-Name": "Z. Spalding",
    "External-Description": "BagIt profile for ingesting content into Southeastern DDA. Updated December 15, 2025.",
    "Source-Organization": "senylrc.org",
    "Version": "2.2"
  },

  "Accept-BagIt-Version": ["0.97", "1.0"],
  "Accept-Serialization": ["application/tar"],
  "Serialization": "required",
  "Allow-Fetch.txt": false,

  "Manifests-Allowed": ["sha512", "sha256", "md5"],
  "Manifests-Required": ["md5"],
  "Tag-Manifests-Allowed": ["sha512", "sha256", "md5"],
  "Tag-Manifests-Required": [],

  "Tag-Files-Allowed": ["*"],
  "Tag-Files-Required": [],

  "BagIt": {
    "BagIt-Version": { "required": true, "default": "1.0" },
    "Tag-File-Character-Encoding": { "required": true, "default": "UTF-8" }
  },

  "Bag-Info": {
    "Source-Organization": { "required": true },

    "Bag-Count": { "required": false },
    "Bag-Group-Identifier": { "required": false },
    "Bagging-Date": { "required": false },
    "Bagging-Software": { "required": false },
    "Internal-Sender-Description": { "required": false },
    "Internal-Sender-Identifier": { "required": false },
    "Payload-Oxum": { "required": false },

    "Bag-Size": { "required": false },
    "Contact-Email": { "required": false },
    "Contact-Name": { "required": false },
    "Contact-Phone": { "required": false },
    "External-Description": { "required": false },
    "External-Identifier": { "required": false },
    "Organization-Address": { "required": false }
  }
}
